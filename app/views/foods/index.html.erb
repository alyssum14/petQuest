<div class='container-fluid text-center'>
  <h1>Food lists</h1>
      <table >
        <tr class=>
          <th>Food Category</th>
          <th>Name</th>
          <th colspan="3"></th>
        </tr>
      
        <% @foods.each do |food| %>
          <tr>
            <td><%= food.foodCategory %></td>
            <td><%= food.name %></td>
            <td><%= link_to 'See Details', food_path(food) %></td>
            <td><%= link_to 'See NutritionFacts',nutritionfacts_path%></td>
            <td><%= link_to 'Add into Recipe', recipes_path%></td>
         </tr>
       <% end %></div>
     </table>
</div> 
<div align="center" id="ctn">
<a href="http://localhost:3000/foods/new" class="btn2">Add new food</a>
<a href="http://localhost:3000/welcome/index" class="btn2">Back</a></div>